{
  "$schema": "https://json.schemastore.org/tsconfig",
  "_version": "20.1.0",

  "compilerOptions": {
    "esModuleInterop": true,
    "lib": ["es2015"],
    "module": "node16",
    "moduleResolution": "Node16",
    "outDir": "out",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es6"
  }
}
